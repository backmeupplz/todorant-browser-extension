{ 
  "manifest_version": 2,
  "name": "Todorant",
  "version": "1.0",
  "description": "Use cognitive psychology to trick your brain into getting things done with the only To Do manager you will ever need.",
  "icons": { 
    "16": "img/Icon-16.png",
    "48": "img/Icon-48.png",
    "128": "img/Icon-128.png"
  },
  "permissions": ["identity", "tabs", "storage", "cookies", "http://localhost/*", "https://localhost/*"],
  "browser_action": {
    "default_title": "todorant"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "short_name": "Todorant",
  "key": "-----BEGIN PUBLIC KEY----------END PUBLIC KEY-----"
}
